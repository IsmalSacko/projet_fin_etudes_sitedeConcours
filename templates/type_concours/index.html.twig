{% extends 'base.html.twig' %}

{% block title %}TypeConcours index{% endblock %}

{% block body %}
    <h1>TypeConcours index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_concour in type_concours %}
            <tr>
                <td>{{ type_concour.id }}</td>
